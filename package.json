{
  "name": "travel-planner-api",
  "version": "1.0.0",
  "description": "GraphQL API for travel planning with city suggestions, weather forecasts, and activity rankings",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["graphql", "travel", "weather", "typescript"],
  "author": "Jose Lopes",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "graphql": "^16.8.1",
    "axios": "^1.6.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.5",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "ts-jest": "^29.1.2",
    "jest": "^29.7.0",
    "axios-mock-adapter": "^1.22.0",
    "nock": "^13.5.0",
    "@types/nock": "^11.1.0"
  }
}
